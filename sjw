-- æ˜¾ç¤ºæ¬¢è¿å¼¹çª—
local Confirmed = false
local Cancelled = false

WindUI:Popup({
    Title = "xicom Pro",
    Icon = "settings",
    IconThemed = true,
    Content = "è¯·é€‰æ‹©æ“ä½œ",
    Buttons = {
        {
            Title = "å–æ¶ˆ",
            Icon = "x",
            Callback = function() 
                Cancelled = true
                return true
            end,
            Variant = "Secondary",
        },
        {
            Title = "è¿›å…¥xicom Pro",
            Icon = "arrow-right",
            Callback = function() 
                Confirmed = true
                return true
            end,
            Variant = "Primary",
        }
    }
})

repeat task.wait() until Confirmed or Cancelled

if Cancelled then
    WindUI:Notify({Title = "æ“ä½œå·²å–æ¶ˆ", Content = "è„šæœ¬æ‰§è¡Œå·²ç»ˆæ­¢", Duration = 2})
    return
end

