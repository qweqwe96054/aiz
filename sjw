-- æ·»åŠ ç¥å¥‡åŠŸèƒ½
local magicalEnabled = false
local magicalConnection
local swearWords = {
    "è‰æ³¥é©¬ï¼",
    "å‚»é€¼ä¸œè¥¿ï¼", 
    "ä½ å¦ˆæ­»äº†ï¼",
    "ç‹—æ—¥çš„ï¼",
    "æ“ä½ å¤§çˆ·ï¼",
    "åºŸç‰©ä¸€ä¸ªï¼",
    "å»æ­»å§ï¼",
    "åƒåœ¾ç©æ„å„¿ï¼",
    "ä½ ç®—ä»€ä¹ˆä¸œè¥¿ï¼",
    "åƒå±å»å§ï¼",
    "æ“ï¼",
    "æ—¥ä½ å¦ˆï¼",
    "æ»šä½ å¦ˆçš„ï¼",
    "ç•œç‰²ä¸å¦‚ï¼",
    "ç‹å…«è›‹ï¼"
}

local function startSwearing()
    if magicalConnection then
        magicalConnection:Disconnect()
    end
    
    magicalConnection = game:GetService("RunService").Heartbeat:Connect(function()
        if magicalEnabled then
            -- éšæœºé€‰æ‹©ä¸€ä¸ªè„è¯
            local randomSwear = swearWords[math.random(1, #swearWords)]
            
            -- åœ¨èŠå¤©ä¸­å‘é€
            local success, err = pcall(function()
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(randomSwear, "All")
            end)
            
            -- å¦‚æœä¸Šé¢å¤±è´¥ï¼Œå°è¯•å¦ä¸€ç§æ–¹æ³•
            if not success then
                pcall(function()
                    game.Players.LocalPlayer:Chat(randomSwear)
                end)
            end
            
            -- éšæœºç­‰å¾…0.5-2ç§’
            wait(math.random(0.5, 2))
        else
            magicalConnection:Disconnect()
        end
    end)
end

Tabs.General:Toggle({
    Title = "ç¥å¥‡æ¨¡å¼",
    Desc = "å¼€å¯åä¼šéšæœºéª‚è„è¯",
    Value = false,
    Callback = function(state)
        magicalEnabled = state
        if state then
            WindUI:Notify({
                Title = "ç¥å¥‡æ¨¡å¼",
                Content = "ç¥å¥‡æ¨¡å¼å·²å¼€å¯ï¼Œå‡†å¤‡å¼€å§‹éª‚äººï¼",
                Duration = 3
            })
            wait(1)
            startSwearing()
        else
            if magicalConnection then
                magicalConnection:Disconnect()
            end
            WindUI:Notify({
                Title = "ç¥å¥‡æ¨¡å¼",
                Content = "ç¥å¥‡æ¨¡å¼å·²å…³é—­",
                Duration = 3
            })
        end
    end
})

-- æ·»åŠ å¤œè§†åŠŸèƒ½
local nightVisionEnabled = false
local originalLightingSettings = {}
local nightVisionConnection

local function enableNightVision()
    -- ä¿å­˜åŸå§‹è®¾ç½®
    originalLightingSettings = {
        Ambient = game.Lighting.Ambient,
        Brightness = game.Lighting.Brightness,
        GlobalShadows = game.Lighting.GlobalShadows,
        OutdoorAmbient = game.Lighting.OutdoorAmbient
    }
    
    -- è®¾ç½®å¤œè§†æ•ˆæœ
    game.Lighting.Ambient = Color3.fromRGB(200, 200, 200)
    game.Lighting.Brightness = 2
    game.Lighting.GlobalShadows = false
    game.Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    
    -- æ·»åŠ è½»å¾®çš„è“ç»¿è‰²è°ƒ
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Name = "WindUI_NightVision"
    colorCorrection.Brightness = 0.1
    colorCorrection.Contrast = 0.2
    colorCorrection.Saturation = 0.5
    colorCorrection.TintColor = Color3.fromRGB(100, 255, 200)
    colorCorrection.Enabled = true
    colorCorrection.Parent = game.Lighting
    
    -- æ·»åŠ è½»å¾®çš„å…‰æ™•æ•ˆæœ
    local bloom = Instance.new("BloomEffect")
    bloom.Name = "WindUI_NightVisionBloom"
    bloom.Intensity = 0.3
    bloom.Size = 24
    bloom.Threshold = 0.8
    bloom.Enabled = true
    bloom.Parent = game.Lighting
end

local function disableNightVision()
    -- æ¢å¤åŸå§‹è®¾ç½®
    if originalLightingSettings.Ambient then
        game.Lighting.Ambient = originalLightingSettings.Ambient
        game.Lighting.Brightness = originalLightingSettings.Brightness
        game.Lighting.GlobalShadows = originalLightingSettings.GlobalShadows
        game.Lighting.OutdoorAmbient = originalLightingSettings.OutdoorAmbient
    end
    
    -- ç§»é™¤æ•ˆæœ
    for _, effect in pairs(game.Lighting:GetChildren()) do
        if effect.Name == "WindUI_NightVision" or effect.Name == "WindUI_NightVisionBloom" then
            effect:Destroy()
        end
    end
end

Tabs.General:Toggle({
    Title = "å¤œè§†æ¨¡å¼",
    Desc = "å¢å¼ºé»‘æš—ç¯å¢ƒçš„å¯è§†åº¦",
    Value = false,
    Callback = function(state)
        nightVisionEnabled = state
        if state then
            enableNightVision()
            
            if nightVisionConnection then
                nightVisionConnection:Disconnect()
            end
            
            nightVisionConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if nightVisionEnabled then
                    game.Lighting.Ambient = Color3.fromRGB(200, 200, 200)
                    game.Lighting.Brightness = 2
                    game.Lighting.GlobalShadows = false
                    
                    if not game.Lighting:FindFirstChild("WindUI_NightVision") then
                        local colorCorrection = Instance.new("ColorCorrectionEffect")
                        colorCorrection.Name = "WindUI_NightVision"
                        colorCorrection.Brightness = 0.1
                        colorCorrection.Contrast = 0.2
                        colorCorrection.Saturation = 0.5
                        colorCorrection.TintColor = Color3.fromRGB(100, 255, 200)
                        colorCorrection.Enabled = true
                        colorCorrection.Parent = game.Lighting
                    end
                    
                    if not game.Lighting:FindFirstChild("WindUI_NightVisionBloom") then
                        local bloom = Instance.new("BloomEffect")
                        bloom.Name = "WindUI_NightVisionBloom"
                        bloom.Intensity = 0.3
                        bloom.Size = 24
                        bloom.Threshold = 0.8
                        bloom.Enabled = true
                        bloom.Parent = game.Lighting
                    end
                else
                    if nightVisionConnection then
                        nightVisionConnection:Disconnect()
                    end
                end
            end)
            
            WindUI:Notify({
                Title = "å¤œè§†æ¨¡å¼",
                Content = "å¤œè§†åŠŸèƒ½å·²å¯ç”¨",
                Duration = 3
            })
        else
            if nightVisionConnection then
                nightVisionConnection:Disconnect()
            end
            disableNightVision()
            
            WindUI:Notify({
                Title = "å¤œè§†æ¨¡å¼",
                Content = "å¤œè§†åŠŸèƒ½å·²å…³é—­",
                Duration = 3
            })
        end
    end
})

-- æ·»åŠ ç®€å•çš„é«˜äº®ç©å®¶åŠŸèƒ½
local highlightEnabled = false
local highlightObjects = {}

Tabs.General:Toggle({
    Title = "é«˜äº®ç©å®¶",
    Desc = "é«˜äº®æ˜¾ç¤ºæ‰€æœ‰ç©å®¶",
    Value = false,
    Callback = function(state)
        highlightEnabled = state
        
        for _, highlight in pairs(highlightObjects) do
            if highlight and highlight.Parent then
                highlight:Destroy()
            end
        end
        highlightObjects = {}
        
        if state then
            for _, player in pairs(game.Players:GetPlayers()) do
                if player.Character then
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "WindUI_Highlight"
                    highlight.Adornee = player.Character
                    
                    if player == game.Players.LocalPlayer then
                        highlight.FillColor = Color3.fromRGB(0, 255, 0)
                        highlight.OutlineColor = Color3.fromRGB(0, 200, 0)
                    else
                        highlight.FillColor = Color3.fromRGB(255, 0, 0)
                        highlight.OutlineColor = Color3.fromRGB(200, 0, 0)
                    end
                    
                    highlight.FillTransparency = 0.3
                    highlight.OutlineTransparency = 0
                    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                    highlight.Parent = player.Character
                    
                    table.insert(highlightObjects, highlight)
                end
            end
            
            game.Players.PlayerAdded:Connect(function(player)
                if highlightEnabled then
                    player.CharacterAdded:Connect(function(character)
                        wait(1)
                        
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "WindUI_Highlight"
                        highlight.Adornee = character
                        highlight.FillColor = Color3.fromRGB(255, 0, 0)
                        highlight.OutlineColor = Color3.fromRGB(200, 0, 0)
                        highlight.FillTransparency = 0.3
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = character
                        
                        table.insert(highlightObjects, highlight)
                    end)
                end
            end)
            
            local localPlayer = game.Players.LocalPlayer
            localPlayer.CharacterAdded:Connect(function(character)
                if highlightEnabled then
                    wait(1)
                    
                    for i, highlight in pairs(highlightObjects) do
                        if highlight and highlight.Parent == character then
                            highlight:Destroy()
                            table.remove(highlightObjects, i)
                            break
                        end
                    end
                    
                    local highlight = Instance.new("Highlight")
                    highlight.Name = "WindUI_Highlight"
                    highlight.Adornee = character
                    highlight.FillColor = Color3.fromRGB(0, 255, 0)
                    highlight.OutlineColor = Color3.fromRGB(0, 200, 0)
                    highlight.FillTransparency = 0.3
                    highlight.OutlineTransparency = 0
                    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                    highlight.Parent = character
                    
                    table.insert(highlightObjects, highlight)
                end
            end)
            
            WindUI:Notify({
                Title = "é«˜äº®ç©å®¶",
                Content = "å·²é«˜äº®æ˜¾ç¤ºæ‰€æœ‰ç©å®¶",
                Duration = 3
            })
        else
            WindUI:Notify({
                Title = "é«˜äº®ç©å®¶",
                Content = "å·²å…³é—­é«˜äº®æ˜¾ç¤º",
                Duration = 3
            })
        end
    end
})

-- æ·»åŠ é‡ç½®æŒ‰é’®
Tabs.General:Button({
    Title = "é‡ç½®æ‰€æœ‰è®¾ç½®",
    Desc = "æ¢å¤é»˜è®¤è®¾ç½®å€¼",
    Callback = function()
        currentWalkSpeed = 16
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = 16
        end
        
        currentJumpPower = 50
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = 50
        end
        
        currentGravity = 196.2
        game.Workspace.Gravity = 196.2
        
        if magicalEnabled then
            magicalEnabled = false
            if magicalConnection then
                magicalConnection:Disconnect()
            end
        end
        
        if nightVisionEnabled then
            nightVisionEnabled = false
            if nightVisionConnection then
                nightVisionConnection:Disconnect()
            end
            disableNightVision()
        end
        
        if highlightEnabled then
            highlightEnabled = false
            for _, highlight in pairs(highlightObjects) do
                if highlight and highlight.Parent then
                    highlight:Destroy()
                end
            end
            highlightObjects = {}
        end
        
        WindUI:Notify({
            Title = "é‡ç½®æˆåŠŸ",
            Content = "æ‰€æœ‰è®¾ç½®å·²æ¢å¤ä¸ºé»˜è®¤å€¼",
            Duration = 3
        })
    end
})

